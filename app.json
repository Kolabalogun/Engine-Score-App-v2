{
  "expo": {
    "name": "Engine Scores",
    "slug": "ESA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-with-bg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-no-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#edeff2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-no-bg.png",
        "backgroundColor": "#edeff2"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ibrahimdevv.ESA"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
           {"icon": "./assets/notification-icon.png"}
    
      ]
    ],
    
    "extra": {
      "eas": {
        "projectId": "e9ce2d52-3d8b-44ee-bc15-df2978ec7cf9"
      }
    }
  }
}
